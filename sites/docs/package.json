{
	"name": "docs",
	"description": "Docs for Paneforge",
	"version": "0.0.3",
	"private": true,
	"scripts": {
		"dev": "concurrently \"pnpm:dev:content\" \"pnpm:dev:svelte\"",
		"dev:content": "contentlayer dev",
		"dev:svelte": "vite dev",
		"build": "contentlayer build && vite build",
		"build:all": "pnpm -w build",
		"build:content": "contentlayer build",
		"build:content-cachebust": "contentlayer build --clearCache",
		"preview": "vite preview",
		"check": "pnpm build:content && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"sync": "svelte-kit sync"
	},
	"license": "MIT",
	"author": {
		"name": "Huntabyte",
		"url": "https://github.com/huntabyte"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/svecosystem/paneforge"
	},
	"devDependencies": {
		"@sveltejs/adapter-cloudflare": "^4.7.2",
		"@sveltejs/kit": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "4.0.0-next.7",
		"@tailwindcss/typography": "^0.5.10",
		"@types/hast": "^3.0.4",
		"@types/mdast": "^4.0.3",
		"autoprefixer": "^10.4.17",
		"clsx": "^2.1.0",
		"concurrently": "^8.2.2",
		"contentlayer": "^0.3.4",
		"mdsx": "^0.0.6",
		"phosphor-svelte": "^1.4.2",
		"postcss": "^8.4.35",
		"postcss-load-config": "^5.0.3",
		"rehype-pretty-code": "^0.13.0",
		"remark-gfm": "^4.0.0",
		"shiki": "^1.1.3",
		"svelte": "^5.0.0-next.262",
		"svelte-check": "^4.0.4",
		"svelte-toolbelt": "^0.4.1",
		"tailwind-merge": "^2.2.1",
		"tailwind-variants": "^0.2.0",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.7.0",
		"typescript": "^5.5.3",
		"unified": "^11.0.4",
		"unist-builder": "^4.0.0",
